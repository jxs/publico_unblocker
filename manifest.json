{
    "manifest_version": 2,

    "name": "Publico paywall unblocker",
    "description": "Extension to unblock Publico newspaper read limit",
    "version": "1.0.3",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.publico.pt/*"],
            "run_at": "document_end",
            "js": ["unblock.js"]
        }
    ]
}
